{
  "version": "1.0.0",
  "name": "@blueprint-pdf/cli",
  "description": "Terminal-based utility for the blueprint-pdf package.",
  "repository": "https://github.com/robbiesymonds/Blueprint.git",
  "author": "Robbie Symonds",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "files": [
    "dist/*"
  ],
  "targets": {
    "main": {
      "context": "node",
      "isLibrary": false,
      "sourceMap": false
    }
  },
  "bin": {
    "blueprint": "./dist/main.js"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "watch": "parcel watch",
    "build": "yarn clean && parcel build"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "parcel": "^2.8.3",
    "typescript": "^5.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0"
  }
}
